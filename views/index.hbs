				                </a>
				            </div>
				            <article class="mix mix_all" style="display: inline-block; opacity: 1;">
				                <div class="image_holder">
				                    <a class="" href="/product/{{this._id}}"><span class="image"><img src="{{ this.imgPath }}" class="" alt=""></span></a>
				                     <span class="text_holder">
				                        <span class="text_outer">
				                            <span class="text_inner">
				                                <p class="qbutton" href="#" >${{this.price}}</p>
				                                <a class="qbutton" id="add-to-cart" href="/cart/{{this._id}}" >Add to cart</a>
				                            </span>
				                        </span> 
				                    </span>
				                </div>
				            </article>
				        </div> 
				    </div>
				{{/contains}}
			{{/each}}
	    </div>

	    <div class="item">
	    	<h2>Cooking oil</h2>
	    	{{#each products}}
				{{#contains this.category "cooking oil"}}
					<div class="col-sm-6 col-md-6 col-lg-3">
				        <div class="projects_holder">
				            <div class="caption">
				                <a class="" href="/product/{{this._id}}">
				                    <h3 class="title">{{this.title}}</h3>
				                </a>
				            </div>
				            <article class="mix mix_all" style="display: inline-block; opacity: 1;">
				                <div class="image_holder">
				                    <a class="" href="/product/{{this._id}}"><span class="image"><img src="{{ this.imgPath }}" class="" alt=""></span></a>
				                     <span class="text_holder">
				                        <span class="text_outer">
				                            <span class="text_inner">
				                                <p class="qbutton" href="#" >${{this.price}}</p>
				                                <a class="qbutton" id="add-to-cart" href="/cart/{{this._id}}" >Add to cart</a>
				                            </span>
				                        </span> 
				                    </span>
				                </div>
				            </article>
				        </div> 
				    </div>
				{{/contains}}
			{{/each}}
	    </div>
	  </div>

	  <!-- Left and right controls -->
	  <a class="left carousel-control" href="#productCarousel" data-slide="prev">
	    <span class="glyphicon glyphicon-chevron-left"></span>
	    <span class="sr-only">Previous</span>
	  </a>
	  <a class="right carousel-control" href="#productCarousel" data-slide="next">
	    <span class="glyphicon glyphicon-chevron-right"></span>
	    <span class="sr-only">Next</span>
	  </a>
	</div>
</div>
</div>