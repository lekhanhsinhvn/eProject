
<div class="container-fluid">
	<div id="slider" class="carousel slide carousel-fade" data-ride="carousel">
		<div class="carousel-inner">
			<div class="board"></div>
			<div class="item active">
				<div class="img-holder">
					<img src="/images/slide1.jpg">
				</div>
				<div class="detail-holder">qwertyuiop</div>
			</div>
			<div class="item">
				<div class="img-holder">
					<img src="/images/slide1.jpg">
				</div>
				<div class="detail-holder">zxcvbnbvczcvc</div>
			</div>
		</div>
	</div>
</div>
<div class="container-fluid">
	<div id="productCarousel" class="carousel slide carousel-fade" data-ride="carousel">
	  <!-- Wrapper for slides -->
	  <div class="carousel-inner">
	    <div class="item active">
	    	<h2>Pulses</h2>
	    	{{#each products}}
				{{#contains this.category "pulses"}}
					<div class="col-sm-6 col-md-6 col-lg-3">
				        <div class="projects_holder">
				            <div class="caption">
				                <a class="" href="/product/{{this._id}}">
				                    <h3 class="title">{{this.title}}</h3>
				                </a>
				            </div>
				            <article class="mix mix_all" style="display: inline-block; opacity: 1;">
				                <div class="image_holder">
				                    <a class="" href="/product/{{this._id}}"><span class="image"><img src="{{ this.imgPath }}" class="" alt=""></span></a>
				                     <span class="text_holder">
				                        <span class="text_outer">
				                            <span class="text_inner">
				                                <p class="qbutton" href="#" >${{this.price}}</p>
				                                <a class="qbutton" id="add-to-cart" href="/cart/{{this._id}}" >Add to cart</a>
				                            </span>
				                        </span> 
				                    </span>
				                </div>
				            </article>
				        </div> 
				    </div>
				{{/contains}}
			{{/each}}
	    </div>

	    <div class="item">
	    	<h2>Spices and condiment</h2>
		    {{#each products}}
				{{#contains this.category "spices and condiment"}}
					<div class="col-sm-6 col-md-6 col-lg-3">
