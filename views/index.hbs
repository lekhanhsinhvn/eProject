<div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
            {{ successMsg }}
        </div>
    </div>
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
	<ol class="carousel-indicators">
	    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
	    <li data-target="#myCarousel" data-slide-to="1"></li>
	    <li data-target="#myCarousel" data-slide-to="2"></li>
 	</ol>	

  <!-- Wrapper for slides -->
  	<div class="carousel-inner">
    	<div class="item active">
      		<img src="/images/slide1.jpeg" alt="Fruit">
      		<div class="header-text hidden-xs">
            	<div class="col-md-12 text-center">
	            	<h2><span>Welcome to <strong>LOREM IPSUM</strong></span></h2>
	            	<br>
	                <h3><span>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</span></h3>
	                <br>
	                <div class="">
                	</div>
            	</div><!-- /header-text -->
   			 </div>
   		</div>
    	<div class="item">
      		<img src="/images/slide2.jpg" alt="Spice">
    	</div>
    	<div class="item">
      		<img src="/images/slide3.png" alt="Cooking Oils">
    	</div>
  	</div>
  <!-- Left and right controls -->
  	<a class="left carousel-control" href="#myCarousel" data-slide="prev">
	    <span class="glyphicon glyphicon-chevron-left"></span>
	    <span class="sr-only">Previous</span>
	</a>
	<a class="right carousel-control" href="#myCarousel" data-slide="next">
	    <span class="glyphicon glyphicon-chevron-right"></span>
	    <span class="sr-only">Next</span>
	</a>
</div>
<div id="home_body">
	<h1>Welcome to Star Organic farm !</h1>
	<ul class="section">
		<li><a href="#"><div class="contain">
			<img src="/images/home_page section shop.jpg">
			<div class="centered">Products</div>
		</div></a></li>
		<li><a href="#"><div class="contain">
			<img src="/images/home_page section aboutus.jpg">
			<div class="centered">About Us</div>
		</div></a></li>
		<li><a href="#"><div class="contain">
			<img src="/images/home_page section gallery.jpg">
			<div class="centered">Gallery</div>
		</div></a></li>
	</ul>
</div>
</div>